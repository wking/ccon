{
  "version": "0.1.0",
  "namespaces": {
    "user": {
      "setgroups": false,
      "uidMappings": [
        {
          "containerID": 0,
          "hostID": 1000,
          "size": 1
        }
      ],
      "gidMappings": [
        {
          "containerID": 0,
          "hostID": 1000,
          "size": 1
        }
      ]
    },
    "net": {}
  },
  "process": {
    "user": {
      "uid": 0,
      "gid": 0
    },
    "capabilities": [
      "CAP_NET_BIND_SERVICE"
    ],
    "args": [
      "nc",
      "-l",
      "-p",
      "80"
    ]
  }
}
