{
	"version": "0.1.0",
	"platform": {
		"os": "linux",
		"arch": "amd64"
	},
	"hostname": "mrsdalloway",
	"root": {
		"path": "rootfs",
		"readonly": true
	},
	"process": {
		"terminal": true,
		"user": {
			"uid": 0,
			"gid": 0
		},
		"env": [
			"PATH=/bin",
			"TERM=xterm"
		],
		"cwd": "/root",
		"args": ["busybox", "sh"]
	},
	"mounts": [
		{
			"name": "root",
			"path": "/root"
		}
	],
	"linux": {
		"capabilities": [
			"CAP_AUDIT_WRITE",
			"CAP_KILL",
			"CAP_NET_BIND_SERVICE"
		],
		"uidMappings": [
			{
				"hostID": 1000,
				"containerID": 0,
				"size": 1
			}
		],
		"gidMappings": [
			{
				"hostID": 1000,
				"containerID": 0,
				"size": 1
			}
		]
	}
}
